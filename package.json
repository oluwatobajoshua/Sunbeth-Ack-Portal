{
  "name": "sunbeth-portal-react-ts",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@azure/msal-browser": "^3.30.0",
    "@azure/msal-react": "^2.2.0",
    "@microsoft/mgt-element": "^4.6.0",
    "@microsoft/mgt-msal2-provider": "^4.6.0",
    "@microsoft/mgt-react": "^4.6.0",
    "axios": "^1.7.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "docx-preview": "0.3.7",
    "express": "^5.1.0",
    "otplib": "^12.0.1",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-pdf": "^7.7.2",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "sql.js": "^1.13.0",
    "sweetalert2": "^11.26.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-router-dom": "^5.3.3",
    "ajv": "^6.12.6",
    "ajv-keywords": "^3.5.2",
  "typedoc": "^0.25.13",
  "typescript": "^4.9.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
  "@typescript-eslint/eslint-plugin": "^5.62.0",
  "@typescript-eslint/parser": "^5.62.0",
    "prettier": "^3.3.3",
    "husky": "^8.0.3",
    "cross-env": "^7.0.3",
    "serve": "^14.2.1"
  },
  "scripts": {
    "prestart": "powershell -NoProfile -ExecutionPolicy Bypass -File ..\\scripts\\manage-backend.ps1 -Action Start",
    "start": "cross-env REACT_APP_ENABLE_SQLITE=true REACT_APP_API_BASE=http://127.0.0.1:4000 react-scripts start",
    "start:plain": "react-scripts start",
  "build": "cross-env GENERATE_SOURCEMAP=false DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "react-scripts test",
    "check:prepush": "npm run lint && npm test --silent -- --watchAll=false && npm run build",
    "preview": "serve -s build -l 5000",
    "serve": "serve -s build -l 5000",
    "fix:sourcemaps": "node scripts/remove-docx-preview-sourcemaps.cjs",
    "postinstall": "node scripts/remove-docx-preview-sourcemaps.cjs",
    "api": "node server/index.js",
    "docs": "typedoc --entryPointStrategy expand --entryPoints src/services src/context src/utils --out docs",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
